generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  name     String @db.VarChar(50)
  surname  String @db.VarChar(50)
  password String @db.VarChar(255)
  email    String @unique @db.VarChar(50)
}
